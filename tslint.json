{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "semicolon": [
      true,
      "always"
    ],
    "indent": [
      true,
      "spaces",
      2
    ]
  },
  "rulesDirectory": []
}